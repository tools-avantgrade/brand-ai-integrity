# Streamlit Secrets Configuration Example
# Copy this file to .streamlit/secrets.toml and fill in your actual values

# REQUIRED: Your Google Gemini API Key
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY = "YOUR_GEMINI_API_KEY_HERE"

# OPTIONAL: Gemini model for generating AI responses
# Default: "gemini-1.5-flash" (fast and cost-effective)
# Alternatives: "gemini-1.5-pro", "gemini-2.0-flash-exp"
GEMINI_MODEL = "gemini-1.5-flash"

# OPTIONAL: Gemini model for evaluation
# Default: same as GEMINI_MODEL
# Use a more capable model for better evaluation accuracy
EVALUATOR_MODEL = "gemini-1.5-flash"
